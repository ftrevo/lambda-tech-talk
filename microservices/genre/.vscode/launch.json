{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Serverless Offline",
            "program": "${workspaceRoot}/node_modules/serverless/bin/serverless",
            "stopOnEntry": false,
            "console": "integratedTerminal",
            "args": [
                "offline",
                "start"
            ],
            "cwd": "${workspaceRoot}",
            "preLaunchTask": null,
            "runtimeExecutable": "node",
            "runtimeArgs": [
                "--nolazy"
            ],
            "sourceMaps": false
        }
    ]
}